{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "email": {
      "type": "email",
      "minLength": 6,
      "required": true
    },
    "phone": {
      "type": "string",
      "minLength": 11,
      "required": true,
      "private": true
    },
    "name": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "required": false
    },
    "releases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::release.release",
      "mappedBy": "user"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": false
    },
    "vk": {
      "type": "string"
    },
    "odnoklassniki": {
      "type": "string"
    },
    "youtube": {
      "type": "string"
    },
    "rutube": {
      "type": "string"
    },
    "telegram": {
      "type": "string"
    },
    "facebook": {
      "type": "string"
    },
    "instagram": {
      "type": "string"
    },
    "site": {
      "type": "string"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "provider": {
      "type": "string"
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false
    },
    "blocked": {
      "type": "boolean",
      "default": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users"
    },
    "username": {
      "type": "string",
      "required": false,
      "unique": false
    }
  }
}
