{
  "kind": "collectionType",
  "collectionName": "releases",
  "info": {
    "singularName": "release",
    "pluralName": "releases",
    "displayName": "Release",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "single",
        "album"
      ],
      "default": "single",
      "required": true
    },
    "artistName": {
      "type": "string"
    },
    "date": {
      "type": "date"
    },
    "img": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "releases"
    },
    "itunes": {
      "type": "string"
    },
    "appleMusic": {
      "type": "string"
    },
    "spotify": {
      "type": "string"
    },
    "yandexMusic": {
      "type": "string"
    },
    "sberZvuk": {
      "type": "string"
    },
    "youtubeMusic": {
      "type": "string"
    },
    "facebookPixel": {
      "type": "string"
    },
    "vkPixel": {
      "type": "string"
    },
    "video": {
      "type": "string"
    },
    "link": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 3
    }
  }
}
