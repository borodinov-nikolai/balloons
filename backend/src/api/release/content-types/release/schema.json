{
  "kind": "collectionType",
  "collectionName": "releases",
  "info": {
    "singularName": "release",
    "pluralName": "releases",
    "displayName": "Release",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "single",
        "album"
      ],
      "default": "single",
      "required": true
    },
    "artistName": {
      "type": "string"
    },
    "date": {
      "type": "date"
    },
    "img": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "releases"
    }
  }
}
